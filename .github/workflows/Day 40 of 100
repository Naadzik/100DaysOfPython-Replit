ContactCard = {}

print("ðŸŒŸðŸŒŸðŸŒŸ Contact Card ðŸŒŸðŸŒŸðŸŒŸ")

print("Input your name")
ContactCard["name"] = input("> ").strip().capitalize()
print()
print("Input your date of birth")
ContactCard["dob"] = input("> ").strip()
print()
print("Input your telephone")
ContactCard["tel"] = input("> ").strip()
print()
print("Input your e-mail address")
ContactCard["email"] = input("> ").strip().lower()
print()
print("Input your home address")
ContactCard["HomeAddress"] = input("> ").strip()
print()
print(f"Hi {ContactCard['name']}! Our dictionary says that you were born on {ContactCard['dob']}. Your telephone number is {ContactCard['tel']} and your e-mail address is {ContactCard['email']}. We deliever your post to {ContactCard['HomeAddress']}. Is that correct?")
print()
print(f"Name: {ContactCard['name']}")
print(f"DOB: {ContactCard['dob']}")
print(f"Tel: {ContactCard['tel']}")
print(f"Email: {ContactCard['email']}")
print(f"Address: {ContactCard['HomeAddress']}")
